1 SELECT SUM(opentdb_id) FROM Category; //sum
2 SELECT AVG(opentdb_id) FROM Category; //avg
3 SELECT AVG(finished_at) FROM _prisma_migrations;1 or 2 or 4 //avg
4 SELECT * FROM Question where question like 'How%';   //like
5 SELECT * FROM Question where question like 'what%';  //like
